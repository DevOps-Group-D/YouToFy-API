{
    "family": "YouToFy-youtube-task",
    "containerDefinitions": [
        {
            "name": "YouToFy-youtube-container",
            "image": "PLACEHOLDER_IMAGE",
            "essential": true,
            "command": ["./YouToFy-API", "youtube", "5002"],
            "portMappings": [
                {
                    "containerPort": 5002,
                    "hostPort": 0,
                    "protocol": "tcp"
                }
            ],
            "memory": 256,
            "cpu": 512,
            "environment": [
                {
                    "name": "POSTGRES_DB",
                    "value": "PLACEHOLDER_POSTGRES_DB"
                },
                {
                    "name": "POSTGRES_USER",
                    "value": "PLACEHOLDER_POSTGRES_USER"
                },
                {
                    "name": "POSTGRES_PASSWORD",
                    "value": "PLACEHOLDER_POSTGRES_PASSWORD"
                },
                {
                    "name": "POSTGRES_PORT",
                    "value": "PLACEHOLDER_POSTGRES_PORT"
                },
                {
                    "name": "BACKEND_DATABASE_URL",
                    "value": "PLACEHOLDER_BACKEND_DATABASE_URL"
                },
                {
                    "name": "BACKEND_PORT",
                    "value": "5002"
                },
                {
                    "name": "YOUTUBE_CLIENT_ID",
                    "value": "PLACEHOLDER_YOUTUBE_CLIENT_ID"
                },
                {
                    "name": "YOUTUBE_CLIENT_SECRET",
                    "value": "PLACEHOLDER_YOUTUBE_CLIENT_SECRET"
                },
                {
                    "name": "YOUTUBE_PROJECT_ID",
                    "value": "PLACEHOLDER_YOUTUBE_PROJECT_ID"
                },
                {
                    "name": "YOUTUBE_AUTH_URI",
                    "value": "PLACEHOLDER_YOUTUBE_AUTH_URI"
                },
                {
                    "name": "YOUTUBE_TOKEN_URI",
                    "value": "PLACEHOLDER_YOUTUBE_TOKEN_URI"
                },
                {
                    "name": "YOUTUBE_AUTH_PROVIDER_X509_CERT_URL",
                    "value": "PLACEHOLDER_YOUTUBE_AUTH_PROVIDER_X509_CERT_URL"
                },
                {
                    "name": "YOUTUBE_REDIRECT_URI",
                    "value": "PLACEHOLDER_YOUTUBE_REDIRECT_URI"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/youtofy",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "youtube"
                }
            }
        }
    ]
}