{
    "family": "youtofy-spotify-task",
    "requiresCompatibilities": ["FARGATE"],
    "networkMode": "awsvpc",
    "cpu": "512",
    "memory": "1024",
    "executionRoleArn": "arn:aws:iam::819080194872:role/youtofy-ecs-task-execution-role",
    "taskRoleArn": "arn:aws:iam::819080194872:role/youtofy-ecs-task-execution-role",
    "containerDefinitions": [
        {
            "name": "youtofy-spotify-container",
            "image": "PLACEHOLDER_IMAGE",
            "essential": true,
            "command": ["./YouToFy-API", "spotify", "5001"],
            "portMappings": [
                {
                    "containerPort": 5001,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "POSTGRES_DB",
                    "value": "PLACEHOLDER_POSTGRES_DB"
                },
                {
                    "name": "POSTGRES_USER",
                    "value": "PLACEHOLDER_POSTGRES_USER"
                },
                {
                    "name": "POSTGRES_PASSWORD",
                    "value": "PLACEHOLDER_POSTGRES_PASSWORD"
                },
                {
                    "name": "POSTGRES_PORT",
                    "value": "PLACEHOLDER_POSTGRES_PORT"
                },
                {
                    "name": "BACKEND_DATABASE_URL",
                    "value": "PLACEHOLDER_BACKEND_DATABASE_URL"
                },
                {
                    "name": "SPOTIFY_CLIENT_ID",
                    "value": "PLACEHOLDER_SPOTIFY_CLIENT_ID"
                },
                {
                    "name": "SPOTIFY_CLIENT_SECRET",
                    "value": "PLACEHOLDER_SPOTIFY_CLIENT_SECRET"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/youtofy",
                    "awslogs-region": "us-east-2",
                    "awslogs-stream-prefix": "spotify"
                }
            }
        }
    ]
}